select first_name, last_name from table_1 t join manager m on (t.id_manager = m.id_manager) where t.dt_work_from = '2020-01-01' and t.dt_work_to = '2020-01-31'
union
select first_name, last_name from table_1 t join employee e on (t.id_employee = e.id_employee)  where t.dt_work_from = '2020-01-01' and t.dt_work_to = '2020-01-31';
